{
	"name": "rcdesign/ttcontent-addfields",
	"type": "typo3-cms-extension",
	"description": "Add CSS Class in Text & Media on Image",
	"license": [
		"GPL-2.0-or-later"
	],
	"homepage": "https://www.rc-design.at",
	"authors": [
		{
			"name": "Christian Racan",
			"email": "werbegrafik@rc-design.at",
			"role": "Developer"
		},
		{
			"name": "Simon Gilli",
			"email": "simon.gilli@gilbertsoft.com",
			"role": "Developer"
		}
	],
	"require": {
		"typo3/cms-core": "^11.5",
		"typo3/cms-fluid-styled-content": "^11.5"
	},
	"require-dev": {
		"ergebnis/composer-normalize": "*",
		"typo3-console/composer-auto-commands": "*",
		"typo3-console/composer-typo3-auto-install": "*",
		"typo3/cms-lowlevel": "*",
		"typo3/coding-standards": "^0.5.2"
	},
	"config": {
		"allow-plugins": {
			"typo3/cms-composer-installers": true,
			"typo3/class-alias-loader": true,
			"ergebnis/composer-normalize": true
		},
		"sort-packages": true
	},
	"extra": {
		"typo3/cms": {
			"extension-key": "ttcontent_addfields"
		}
	}
}